{
    "name": "@irimold/react-component",
    "version": "0.24.0",
    "description": "Irimold React Component",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./classes": {
            "types": "./dist/classes.d.ts",
            "default": "./dist/classes.js"
        },
        "./constants": {
            "types": "./dist/constants.d.ts",
            "default": "./dist/constants.js"
        },
        "./style": "./dist/style.css",
        "./types": {
            "types": "./dist/src/types/index.d.ts"
        },
        "./tailwindcss-config": {
            "types": "./dist/tailwindcss-config.d.ts",
            "default": "./dist/tailwindcss-config.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Irimold/react-component.git"
    },
    "scripts": {
        "build": "tsc && vite build",
        "lint": "eslint src --max-warnings=0"
    },
    "keywords": [
        "React Component"
    ],
    "author": {
        "name": "Irimold",
        "email": "hello@irimold.web.id",
        "url": "https://irimold.web.id"
    },
    "license": "MIT",
    "devDependencies": {
        "@eslint/compat": "^1.2.4",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.17.0",
        "@irimold/react-hooks": "^0.3.0",
        "@irimold/react-icons": "^0.9.0",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@typescript-eslint/eslint-plugin": "^8",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.16",
        "eslint": "^9",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^5",
        "eslint-plugin-tailwindcss": "^3.13.1",
        "globals": "^15.14.0",
        "postcss": "^8.4.32",
        "react": "^19",
        "react-dom": "^19",
        "rollup-plugin-preserve-directives": "^0.4.0",
        "tailwindcss": "^3.4.0",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
        "vite": "^6",
        "vite-plugin-dts": "^4.0.0"
    },
    "peerDependencies": {
        "@irimold/react-hooks": "^0.3.0",
        "@irimold/react-icons": "^0.9.0",
        "@types/react": "^19",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.32",
        "react": "^19",
        "react-dom": "^19",
        "tailwindcss": "^3.4.0"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        },
        "@types/react-dom": {
            "optional": true
        }
    },
    "sideEffects": false,
    "files": [
        "dist"
    ]
}
